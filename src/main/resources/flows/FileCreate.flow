{
  "id": "93c8952f-a858-4ad9-ac23-4e5e30d92871",
  "title": "GET Hello world flow",
  "description": "Returns 'Hello world' text from GET /api/message",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "GET Hello World REST Listener",
      "configuration": {"ref": "f1dd4761-2578-4506-a51f-881e1e5a7945"},
      "path": "/create",
      "method": "POST",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.google.drive.v3.component.FileCreate",
      "description": "Google File Create",
      "configuration": {"ref": "314f4dc1-5809-4e3d-934e-03519709d619"},
      "mimeType": "text/plain"
    },
    {
      "implementor": "com.reedelk.google.drive.v3.component.PermissionCreate",
      "description": "Google Permission Create",
      "configuration": {"ref": "314f4dc1-5809-4e3d-934e-03519709d619"},
      "fileId": "#[message.payload()]",
      "type": "GROUP",
      "emailAddress": ""
    }
  ]
}