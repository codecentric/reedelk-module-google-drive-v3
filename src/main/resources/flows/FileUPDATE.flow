{
  "id": "bfe578fa-50cb-4820-8a37-8cd1eade6d40",
  "title": "File UPDATE",
  "description": "File UPDATE Flow",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "REST Listener",
      "configuration": {"ref": "f1dd4761-2578-4506-a51f-881e1e5a7945"},
      "path": "/file/{fileId}",
      "method": "PUT",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.google.drive.v3.component.FileUpdate",
      "description": "Drive File Update",
      "configuration": {"ref": "314f4dc1-5809-4e3d-934e-03519709d619"},
      "fileId": "#[message.attributes().pathParams.fileId]",
      "mimeType": "text/plain"
    }
  ]
}