{
  "id": "3cbe97f9-fda2-4d81-b6cf-c3ef0c5131bd",
  "title": "File DELETE",
  "description": "File DELETE Flow",
  "flow": [
    {
      "implementor": "com.reedelk.rest.component.RESTListener",
      "description": "REST Listener",
      "configuration": {"ref": "f1dd4761-2578-4506-a51f-881e1e5a7945"},
      "path": "/file/{fileId}",
      "method": "DELETE",
      "streaming": "AUTO",
      "response": {
        "body": "#[message.payload()]",
        "status": 200
      },
      "errorResponse": {
        "body": "#[error]",
        "status": 500
      }
    },
    {
      "implementor": "com.reedelk.google.drive.v3.component.FileDelete",
      "description": "Drive File Delete",
      "configuration": {"ref": "314f4dc1-5809-4e3d-934e-03519709d619"},
      "fileId": "#[message.attributes().pathParams.fileId]"
    }
  ]
}